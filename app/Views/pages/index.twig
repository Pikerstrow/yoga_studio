<!-- News -->
<section class="py-5" id="News">
    <div class="container">
        <div class="row">
            <div class="col-lg-5 col-md-12 mb-3 text-center">
                <div class="container-block wow fadeInLeft">
                    <img src="images/news/news.jpg" class="img-fluid">
